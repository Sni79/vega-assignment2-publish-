{

  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",

  "data": {"url": "https://raw.githubusercontent.com/Sni79/vega-assignment2/master/inf_waste.csv"},
   "description": "A vertical 2D box plot showing median, min, and max in the Solid waste in 2018",
  "width": 700,
  "height": 450,
  

  "mark": {
    "type": "boxplot",
    "extent": "min-max"
  },
  "encoding": {
    "x": {"field": "dname", "type": "ordinal"},
    "y": {
      "field": "wasteamt",
      "type": "quantitative",
      "axis": {"title": "wasteamt"},
      
    "tooltip": [{"field": "dcode", "type": "nominal"},

                {"field": "wasteamt", "type": "quantitative"}]
    }
  }
}