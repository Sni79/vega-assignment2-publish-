{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {
    "url": "https://raw.githubusercontent.com/Maplub/AirQualityData/master/PM10_2018_daily.csv"
  },
  "mark": "line",
  "width": 500,
  "height": 450,
  "transform": [
    {
      "filter": {
        "field": "pm10",
        "range": [
          0,
          150
        ]
      }
    }
  ],
  "encoding": {
    "x": {
      "timeUnit": "yearmonthday",
      "field": "time",
      "type": "temporal"
    },
    "title": "time",
    "scale": {
      "padding": 0.5
    },
    "y": {
      "aggregate": "mean",
      "field": "pm10",
      "type": "quantitative",
      "title": "pm10",
      "color": {
        "field": "site",
        "type": "nominal"
      }
    }
  },
  "config": {}
}